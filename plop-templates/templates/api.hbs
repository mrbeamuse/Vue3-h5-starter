import { http } from "@/utils/http";
// import {} from "./types";

export const {{ apiName }}ListApi = (params) => {
  return http.request({
    url: "/{{ apiName }}/list",
    method: "get",
    params
  });
};

export const {{ apiName }}UpdateApi = (data) => {
  return http.request({
    url: "/{{ apiName }}/update",
    method: "post",
    data
  });
};